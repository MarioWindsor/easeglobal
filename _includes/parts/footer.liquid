<script>
	/* Form 1 */
	document.addEventListener("DOMContentLoaded", function() {
	    var form = document.getElementById("egImmigrationLeads");
	    form.addEventListener("submit", function(event) {
	        event.preventDefault(); // Prevent default form submission
	        
	        // Collect form data
	        var formData = new FormData(form);
	        
	        // Convert formData to URL encoded string
	        var urlEncodedData = new URLSearchParams(formData).toString();
	        
	        // Define endpoint URL
	        var endpoint = "https://docs.google.com/forms/u/0/d/e/1FAIpQLScwVGA7s0ndpD6lX7rQNbFu0yQvPlEZrwwYYkqmebG2FJFR2g/formResponse";
	        
	        // Fetch API POST request
	        fetch(endpoint, {
	            method: "POST",
	            mode: "cors",
	            credentials: "include",
	            headers: {
	                "Content-Type": "application/x-www-form-urlencoded"
	            },
	            body: urlEncodedData
	        })
	        .then(response => {
	            if (!response.success) {
	                throw new Error("Network response was Unsuccessful");
	            }
	            return response.text();
	        })
	        .then(data => {
	            // Handle success response
	            console.log("success:", data);
	            // You can handle the success response here
	            // For example, display a success message to the user
	        })
	        .catch(error => {
	        	if ( error instanceof TypeError ) {
	        		// This error occurs due to CORS (which is a browser security feature),
	        		// 	however Google has received the submission successfully
	        		// Provide feedback to user
	        		form.classList.add("thank-you");
	        		return
	        	}
	        	// Legitimate error has occurred.
	        	// Handle error and report back to user accordingly
	            console.error("error:", error);
	        	// Form Fail
	            form.classList.add("fail");
	        });
	    });
	});
</script>

